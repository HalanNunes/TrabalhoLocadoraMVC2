@{
    TrabalhoLocadoraMVC2.Models.Repository db = new TrabalhoLocadoraMVC2.Models.Repository();
    var list = db.TipoCopias.OrderBy(c => c.Descricao).ToList();
    var selectList = new SelectList(list, "Id", "Descricao", Model);     
}

@Html.DropDownListFor(model => model, selectList, "[Selecione]");